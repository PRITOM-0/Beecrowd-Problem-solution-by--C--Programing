#include<stdio.h>
int main()

{

int n,a,i;
scanf("%d",&n);

for(i=0;i<n;i++){
    scanf("%d",&a);

    if(a==0){
        printf("NULL\n");
    }
    else if(a%2==0){
        if(a<0){
            printf("EVEN NEGATIVE\n");
        }else{
            printf("EVEN POSITIVE\n");
        }

    }
    else if(a%2!=0){
        if(a<0){
            printf("ODD NEGATIVE\n");
        }else{
            printf("ODD POSITIVE\n");
        }

    }
}

return 0;

}
