#include <stdio.h>

int main() {

   int I,J;
   for(I=1,J=7;J>4;J--){
       printf("I=%d J=%d\n",I,J);
   }
      for(I=3,J=7;J>4;J--){
       printf("I=%d J=%d\n",I,J);
   }
      for(I=5,J=7;J>4;J--){
       printf("I=%d J=%d\n",I,J);
   }
   for(I=7,J=7;J>4;J--){
       printf("I=%d J=%d\n",I,J);
   }
      for(I=9,J=7;J>4;J--){
       printf("I=%d J=%d\n",I,J);
   }
    return 0;
}
