#include <stdio.h>

int main() {

    int a,b,d;
    scanf("%d%d",&a,&b);
    if(a<b){
        d=b-a;
        printf("O JOGO DUROU %d HORA(S)\n",d);
    }
    else if(a>b){
        d=(24-a)+b;
        printf("O JOGO DUROU %d HORA(S)\n",d);
    }
    else if(a==b){
        d=24;
        printf("O JOGO DUROU %d HORA(S)\n",d);
    }
    /*else if(a>12&&b<12){
        d=(24-a)+b;
        printf("O JOGO DUROU %d HORA(S)\n",d);
    }
    else if(a==0&&b==0){

        printf("O JOGO DUROU 24 HORA(S)\n",d);
    }*/


    return 0;
}
