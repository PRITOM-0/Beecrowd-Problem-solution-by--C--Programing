#include <stdio.h>
 
int main() {
 
   int a,b,c,d,e,E=0,O=0,P=0,N=0;
   scanf("%d%d%d%d%d",&a,&b,&c,&d,&e);
   
   if(a%2==0){
       E++;
       if(a>0){P++;}else if(a<0){N++;}
   }else{
       O++;
       if(a>0){P++;}else if(a<0){N++;}
   }
   if(b%2==0){
       E++;
       if(b>0){P++;}else if(b<0){N++;}
   }else{
       O++;
       if(b>0){P++;}else if(b<0){N++;}
   }
   if(c%2==0){
       E++;
       if(c>0){P++;}else if(c<0){N++;}
   }else{
       O++;
       if(c>0){P++;}else if(c<0){N++;}
   }
   if(d%2==0){
       E++;
       if(d>0){P++;}else if(d<0){N++;}
   }else{
       O++;
       if(d>0){P++;}else if(d<0){N++;}
   }
   if(e%2==0){
       E++;
       if(e>0){P++;}else if(e<0){N++;}
   }else{
       O++;
       if(e>0){P++;}else if(e<0){N++;}
   }
   
   printf("%d valor(es) par(es)\n",E);
   printf("%d valor(es) impar(es)\n",O);
   printf("%d valor(es) positivo(s)\n",P);
   printf("%d valor(es) negativo(s)\n",N);
   
   
    return 0;
}
