#include <stdio.h>
 
int main() {
 
   double a,b,c,d,t,R1,R2;
   scanf("%lf%lf%lf",&a,&b,&c);
   t=(b*b) - (4*a*c);
   d=sqrt((b*b) - (4*a*c));
   R1= (-b+d)/(2*a);
   R2= (-b-d)/(2*a);
   
   if(t<0 || a==0){
       printf("Impossivel calcular\n");
   }
   else{
       printf("R1 = %0.5lf\nR2 = %0.5lf\n",R1,R2);
   }
    return 0;
}
